{
    "name": "odatatools-cli",
    "displayName": "OData Client Generator",
    "description": "Tool for generating client for OData Webservices",
    "version": "1.0.1",
    "publisher": "erwinsmit",
    "main": "dist/cli.js",
    "bin": "dist/cli.js",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "compile": "tsc && ncp src/templates dist/templates",
        "createProxy": "node ./dist/cli.js --outDir=exampledir --endpoint=http://localhost:32323/odata/$metadata"
    },
    "devDependencies": {
        "@types/fs-extra": "4.0.5",
        "@types/handlebars": "4.0.36",
        "@types/ncp": "2.0.1",
        "@types/node": "8.0.55",
        "@types/request": "^2.0.9",
        "@types/minimist": "^1.2.0",
        "typescript": "3.8.3"
    },
    "dependencies": {
        "ncp": "^2.0.0",
        "fs-extra": "9.0.0",
        "handlebars": "4.0.11",
        "minimist": "^1.2.5",
        "node-rest-client": "3.1.0",
        "request": "2.83.0",
        "vscode": "1.1.10",
        "xml2js": "0.4.19"
    },
    "author": {
        "email": "erwin.smit@gmail.com",
        "name": "Erwin Smit"
    },
    "keywords": [
        "oData",
        "proxyclient"
    ],
    "license": "MIT",
    "bugs": "https://github.com/erwinsmit/odatatools/issues",
    "repository": "https://github.com/erwinsmit/odatatools"
}
